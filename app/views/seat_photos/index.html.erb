<h2>Seat Photos</h2>

<% @seat_photos.each do |seat_photo| %>
  <div>
    <%= image_tag seat_photo.image, :size => "400" %>
    <br><%= seat_photo.seat.section %>, Row: <%= seat_photo.seat.seat_row %>, Seat: <%= seat_photo.seat.seat_number%>
    <br><em>"<%= seat_photo.caption %>"</em>
  <div>



<% end %>
